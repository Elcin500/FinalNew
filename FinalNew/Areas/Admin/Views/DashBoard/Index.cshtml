@model DashBoardViewModel
@{
    ViewData["Title"] = "Index";
}



<div class="content sm-gutter">
    <div class="page-title"></div>
    <!-- END DASHBOARD TILES -->
    <div class="row">
       
        <!-- END WORLD MAP WIDGET - CRAFT MAP -->
        <!-- BEGIN REALTIME SALES GRAPH -->
        <div class="col-md-4 col-vlg-4 m-b-10 ">
            <div class="tiles white">
                <div class="row">
                    <div class="sales-graph-heading">
                        <div class="col-md-5 col-sm-5">
                            <h5 class="no-margin">Total Home Count</h5>
                            <h4>@Model.HomeCount</h4>
                        </div>
                        <div class="col-md-3 col-sm-3">
                            <p class="semi-bold">TODAY</p>
                            <h4>@Model.TodayHomeCount</h4>
                        </div>
                        <div class="col-md-4 col-sm-3">
                            <p class="semi-bold">THIS MONTH</p>
                            <h4>@Model.LastMonthHomeCount</h4>
                        </div>
                        <div class="clearfix"></div>
                    </div>
                </div>
                <h5 class="semi-bold m-t-30 m-l-30">@Model.ip.ToString()</h5>
                <table class="table no-more-tables m-t-20 m-l-20 m-b-30">
                    <thead style="display:none">
                        <tr>
                            <th style="width:9%">Project Name</th>
                            <th style="width:22%">Description</th>
                            <th style="width:6%">Price</th>
                            <th style="width:1%"> </th>
                        </tr>
                    </thead>
                    <tbody>
                        <tr>
                            <td class="v-align-middle bold text-success">Agent Count</td>
                            <td class="v-align-middle"><span class="muted">--------------------</span> </td>
                            <td><span class="muted bold text-success">@Model.AgentCount</span> </td>
                            <td class="v-align-middle"></td>
                        </tr>
                        <tr>
                            <td class="v-align-middle bold text-success">Client Count</td>
                            <td class="v-align-middle"><span class="muted">--------------------</span> </td>
                            <td><span class="muted bold text-success">@Model.ClientCount</span> </td>
                            <td class="v-align-middle"></td>
                        </tr>
                        <tr>
                            <td class="v-align-middle bold text-success">Subscribes Count</td>
                            <td class="v-align-middle"><span class="muted">--------------------</span> </td>
                            <td><span class="muted bold text-success">@Model.SubscribesCount</span> </td>
                            <td class="v-align-middle"></td>
                        </tr>
                    </tbody>
                </table>
                <div id="sales-graph"> </div>
            </div>
        </div>
        <!-- END REALTIME SALES GRAPH -->
    </div>
</div>
